{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Database": "Data Source=(LocalDb)\\MSSQLLocalDB;Initial Catalog=Messenger;Integrated Security=True",
    "AzureStorage": "UseDevelopmentStorage=true;"
  },
  "Azure": {
    "AvatarImageContainerName": "avatars"
  },
  "Jwt": {
    "Issuer": "Messenger",
    "Audience": "Messenger",
    "Key": "<HS512 generated key>"
  }
}
